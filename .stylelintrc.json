  "extends": ["stylelint-config-rational-order"],
  "plugins": [
    "stylelint-order", 
    "stylelint-scss",
    "stylelint-config-rational-order/plugin"
  ],
  "rules": {
    "indentation": 2,
    "selector-class-pattern": null,
    "no-empty-source": true
  },
  "customSyntax": "postcss-scss"
}